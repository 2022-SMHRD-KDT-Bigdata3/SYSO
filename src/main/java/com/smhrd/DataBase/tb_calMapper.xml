<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tb_calMapper">
	<!-- namespace: mapper 구별 / id : 각 sql 구별 -->
	<!-- type 알리아스(별칭) : com.smhrd.model.MsgMemberVO 이 경로에 대해...config에서 지정가능 -->
	<insert id="insertEvent2" parameterType="tb_calVO">
		INSERT INTO tb_calendar values(calendar_seq.nextval,#{user_id},#{groupid},#{title},#{writer},#{content},#{start1},#{end1},#{allday},#{textcolor},#{backgroundcolor},#{bordercolor})
	</insert>
	
	<insert id="insertEvent" parameterType="com.smhrd.DataBase.tb_calVO">
		INSERT INTO tb_calendar values(calendar_seq.nextval,#{user_id},#{title},#{content})
	</insert>
	
</mapper>